{
    "name": "sassnowski/venture-dashboard",
    "description": "A dashboard to visualize your workflows",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kai Sassnowski",
            "email": "me@kai-sassnowski.com"
        }
    ],
    "require": {
        "sassnowski/venture": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sassnowski\\Venture\\Dashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sassnowski\\Venture\\Dashboard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sassnowski\\Venture\\Dashboard\\DashboardServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^6.0",
        "spatie/phpunit-watcher": "^1.23.1",
        "phpunit/phpunit": "^9.5"
    }
}
